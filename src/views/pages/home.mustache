{{>partials/header}}
    <h1>Produtos</h1>

    <ul>
        {{#produtos}}
            <li>{{title}} Preço: {{price}}</li>
        {{/produtos}}
    </ul>

    <hr>
    <h1>Produtos R$ 5,00 + </h1>

    {{#verificacaoPreco}}
        <ul>
            {{#expressionList}}
                <li>{{title}} Preço: R$ {{price}}</li>
            {{/expressionList}}

        </ul>

    {{/verificacaoPreco}}

    <h1>Frases Mc Poze</h1>
    <ul>
        {{#frases}}
            <li>{{.}}</li>
        {{/frases}}
        {{^frases}}
            <li>Hoje não tem frases do Mc Poze</li>
        {{/frases}}
    </ul>
   
{{>partials/footer}}